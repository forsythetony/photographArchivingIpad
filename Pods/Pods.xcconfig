CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/CRToast" "${PODS_ROOT}/Headers/ChameleonFramework" "${PODS_ROOT}/Headers/Colours" "${PODS_ROOT}/Headers/FontAwesomeKit" "${PODS_ROOT}/Headers/ImageTaker" "${PODS_ROOT}/Headers/LiveFrost" "${PODS_ROOT}/Headers/Masonry" "${PODS_ROOT}/Headers/SBJson" "${PODS_ROOT}/Headers/VBFPopFlatButton" "${PODS_ROOT}/Headers/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/CRToast" -isystem "${PODS_ROOT}/Headers/ChameleonFramework" -isystem "${PODS_ROOT}/Headers/Colours" -isystem "${PODS_ROOT}/Headers/FontAwesomeKit" -isystem "${PODS_ROOT}/Headers/ImageTaker" -isystem "${PODS_ROOT}/Headers/LiveFrost" -isystem "${PODS_ROOT}/Headers/Masonry" -isystem "${PODS_ROOT}/Headers/SBJson" -isystem "${PODS_ROOT}/Headers/VBFPopFlatButton" -isystem "${PODS_ROOT}/Headers/pop"
OTHER_LDFLAGS = -ObjC -lc++ -framework Accelerate -framework CoreGraphics -framework CoreText -framework Foundation -framework QuartzCore -framework UIKit
PODS_ROOT = ${SRCROOT}/Pods